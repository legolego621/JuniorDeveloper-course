#include <iostream>
using namespace std;
int main(void)
{
	size_t size;
	cout << "Enter count num: ";
	cin >> size;
	if(size > 0)
	{
		int *iValue = new int [size];\
		int resultValue = 0;
		for(size_t i = 0 ; i < size; ++i)
		{
			if(i == 0 || i == 1)
			{
				iValue[0] = iValue[1] = 1;
				resultValue = 1;
			}
			else if(i >= 2)
			{
				iValue[i] = iValue[i - 1] + iValue[i - 2];
				resultValue = iValue[i];
			}
		}
		delete [] iValue;
		cout << "Result value = " << resultValue;
	}
	return 0;
}
